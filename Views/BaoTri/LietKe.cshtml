@model List<object>
@{
}

<table>
    <tr>
        <td>Tên nhân viên</td>
        <td>Số điện thoại</td>
        <td>Số lần</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.GetType().GetProperty("tenNhanVien").GetValue(item, null)</td>
            <td>@item.GetType().GetProperty("soDienThoai").GetValue(item, null)</td>
            <td>@item.GetType().GetProperty("soLan").GetValue(item, null)</td>
        </tr>
    }
</table>

